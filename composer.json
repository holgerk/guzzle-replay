{
    "name": "holgerk/guzzle-replay",
    "description": "A replay middleware for easier testing guzzle clients",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Holgerk\\GuzzleReplay\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "Holgerk\\GuzzleReplay\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Holger Kohnen",
            "email": "h.kohnen@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7",
        "sebastian/diff": "^5||^6",
        "symfony/var-exporter": "^5||^6||^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9||^10||^11",
        "symfony/var-dumper": "^6||^7",
        "symfony/process": "^6||^7",
        "holgerk/assert-golden": "^2.0.6",
        "vimeo/psalm": "^5",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/collections": "^10.48",
        "illuminate/http": "^10.48"
    },
    "scripts": {
        "test:unit": "phpunit tests"
    }
}
